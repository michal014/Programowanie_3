<Window x:Class="Projekt.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt"
        mc:Ignorable="d"
        Title="Rejestracja" Height="300" Width="400">
    <Canvas>
        <Label Content="Login" FontSize="20" Canvas.Left="50" Canvas.Top="30"/>
        <TextBox Text="{Binding Path=login,Mode=TwoWay}" Height="30" FontSize="20" Width="300" Canvas.Left="50" Canvas.Top="70"/>
        <Label Content="Hasło" FontSize="20" Canvas.Left="50" Canvas.Top="110"/>
        <PasswordBox x:Name="passwordbox" Height="30" FontSize="20" Width="300" Canvas.Left="50" Canvas.Top="150"/>
        <Button Click="RegisterBtn" Content="Zarejestruj" Height="40" FontSize="20" Width="120" Canvas.Left="230" Canvas.Top="200"/>
        <Button Click="LoginBtn" Content="Logowanie" Height="40" FontSize="20" Width="120" Canvas.Left="50" Canvas.Top="200"/>
    </Canvas>
</Window>
